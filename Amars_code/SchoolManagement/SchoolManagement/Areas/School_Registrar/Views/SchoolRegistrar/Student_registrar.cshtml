
@model SchoolManagement.Areas.School_Registrar.Models.StudentInformation
@{
    ViewBag.Title = "Student_registrar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<style>
    table, td, th {
        border: 1px solid green;
        border-collapse: collapse;
        width: 30%;
    }

    th {
        border: 1px solid black;
        background-color: green;
        color: white;
    }

    h3, h4 {
        margin: 10px 0 30px 0;
        font-size: 20px;
        color: cornflowerblue;
    }
</style>
<h3>Student Information System</h3>
<h3><p>Enter Class Detail</p></h3>
@using (Html.BeginForm("Contact", "SchoolRegistrar", FormMethod.Post))
{

    <label>Enter Class ID</label>
    @Html.TextBox("text");
    <button type="submit" class="btn btn-default">Submit</button>

}
